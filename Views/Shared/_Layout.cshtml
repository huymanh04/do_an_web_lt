    <script>

        function checkNotification() {
            const notification = document.querySelector('.Thong_bao_chao_mung');
            const content = document.getElementById('center');

            if (notification && notification.offsetParent !== null) {
                // Nếu thông báo hiển thị
                content.style.marginTop = '150px'; // Đặt margin-top thành 120px
            } else {
                // Nếu thông báo không hiển thị
                content.style.marginTop = '0'; // Quay lại vị trí ban đầu
            }
        }

        // Kiểm tra liên tục mỗi 500ms
        setInterval(checkNotification, 500);

        const navbarToggler = document.querySelector('.navbar-toggler');
        const navbarNav = document.getElementById('navbarNav');

        // Hàm kiểm tra trạng thái aria-expanded và thay đổi màu nền
        function checkExpanded() {
            const isExpanded = navbarToggler.getAttribute('aria-expanded') === 'true';

            // Thêm hoặc xóa class dựa vào giá trị aria-expanded
            if (isExpanded) {

                navbarNav.classList.add('expanded');
            } else {

                navbarNav.classList.remove('expanded');
            }
        }

        // Lặp lại kiểm tra sau mỗi 500ms
        setInterval(checkExpanded, 50);


        const currentYear = new Date().getFullYear();

        // Hiển thị năm vào phần tử có id "current-year"
        document.getElementById("copyright-footer").innerHTML = "Copyright " + currentYear + " © Tiệm Hoa 5 Anh Em Siêu Nhân - tiẹmhoa5anhemsiunhan.com";
        var header = document.getElementById("myHeader");

        // Lưu vị trí ban đầu của header
        var sticky = header.offsetTop;

        // Thêm sự kiện cuộn
        window.onscroll = function () {
            if (window.pageYOffset > sticky) {
                header.classList.add("sticky");
            } else {
                header.classList.remove("sticky");
            }
        };
    </script>
